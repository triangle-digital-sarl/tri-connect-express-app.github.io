{
		"name": "@tri/tri-connect-documentation",
		"version": "1.0.0",
		"main": "index.js",
		"repository": "git@github.com:triangle-digital-sarl/tri-connect-express-app.github.io.git",
		"author": "Fabrice Dufils Siyapdje <dev.dafab@gmail.com>",
		"license": "MIT",
		"scripts": {
				"vite": "vite",
				"smui-theme": "smui-theme",
				"start": "vite dev",
				"build": "rimraf docs && vite build",
				"postbuild": "mv build docs",
				"redocly": "redocly",
				"mermaid": "mmdc",
				"test": "npm run test:integration && npm run test:unit",
				"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
				"test:integration": "playwright test",
				"test:unit": "vitest",
				"theme": "npm run theme-smui && npm run theme-fixation && npm run theme-material",
				"theme-smui": "npm run smui-theme-light && npm run smui-theme-dark && npm run site-light && npm run site-dark",
				"theme-fixation": "npm run smui-fixation-light && npm run smui-fixation-dark && npm run site-fixation-light && npm run site-fixation-dark",
				"theme-material": "npm run smui-material-light && npm run smui-material-dark && npm run site-material-light && npm run site-material-dark",
				"smui-theme-light": "smui-theme compile static/smui.css -s expanded -i src/theme -i node_modules/",
				"smui-theme-dark": "smui-theme compile static/smui-dark.css -s expanded -i src/theme/dark -i node_modules/",
				"smui-fixation-light": "smui-theme compile static/smui-fixation.css -s expanded -i src/theme/fixation -i node_modules/",
				"smui-fixation-dark": "smui-theme compile static/smui-fixation-dark.css -s expanded -i src/theme/fixation-dark -i node_modules/",
				"smui-material-light": "smui-theme compile static/smui-material.css -s expanded -i src/theme/material -i node_modules/",
				"smui-material-dark": "smui-theme compile static/smui-material-dark.css -s expanded -i src/theme/material-dark -i node_modules/",
				"site-light": "sass --no-source-map -I src/theme -I node_modules -I ../smui-theme/node_modules -I ../../node_modules src/site.scss static/site.css",
				"site-dark": "sass --no-source-map -I src/theme/dark -I node_modules -I ../smui-theme/node_modules -I ../../node_modules src/site.scss static/site-dark.css",
				"site-fixation-light": "sass --no-source-map -I src/theme/fixation -I node_modules -I ../smui-theme/node_modules -I ../../node_modules src/site.scss static/site-fixation.css",
				"site-fixation-dark": "sass --no-source-map -I src/theme/fixation-dark -I node_modules -I ../smui-theme/node_modules -I ../../node_modules src/site.scss static/site-fixation-dark.css",
				"site-material-light": "sass --no-source-map -I src/theme/material -I node_modules -I ../smui-theme/node_modules -I ../../node_modules src/site.scss static/site-material.css",
				"site-material-dark": "sass --no-source-map -I src/theme/material-dark -I node_modules -I ../smui-theme/node_modules -I ../../node_modules src/site.scss static/site-material-dark.css"
		},
		"devDependencies": {
				"@mdi/js": "^7.4.47",
				"@mermaid-js/mermaid-cli": "^10.8.0",
				"@playwright/test": "^1.28.1",
				"@redocly/cli": "^1.9.1",
				"@smui-extra/accordion": "^7.0.0-beta.16",
				"@smui-extra/autocomplete": "^7.0.0-beta.16",
				"@smui-extra/badge": "^7.0.0-beta.16",
				"@smui-extra/bottom-app-bar": "^7.0.0-beta.16",
				"@smui/button": "^7.0.0-beta.16",
				"@smui/card": "^7.0.0-beta.16",
				"@smui/circular-progress": "^7.0.0-beta.16",
				"@smui/data-table": "^7.0.0-beta.16",
				"@smui/drawer": "^7.0.0-beta.16",
				"@smui/fab": "^7.0.0-beta.16",
				"@smui/layout-grid": "^7.0.0-beta.16",
				"@smui/linear-progress": "^7.0.0-beta.16",
				"@smui/list": "^7.0.0-beta.16",
				"@smui/menu": "^7.0.0-beta.16",
				"@smui/menu-surface": "^7.0.0-beta.16",
				"@smui/paper": "^7.0.0-beta.16",
				"@smui/switch": "^7.0.0-beta.16",
				"@smui/tab": "^7.0.0-beta.16",
				"@smui/tab-bar": "^7.0.0-beta.16",
				"@smui/top-app-bar": "^7.0.0-beta.16",
				"@sveltejs/adapter-auto": "^3.0.0",
				"@sveltejs/adapter-static": "^3.0.1",
				"@sveltejs/kit": "^2.0.0",
				"@sveltejs/vite-plugin-svelte": "^3.0.0",
				"@theguild/remark-mermaid": "^0.0.6",
				"@tri/sdk": "*",
				"@types/eslint": "^8.56.0",
				"@typescript-eslint/eslint-plugin": "^7.0.0",
				"@typescript-eslint/parser": "^7.0.0",
				"eslint": "^8.56.0",
				"eslint-config-prettier": "^9.1.0",
				"eslint-plugin-svelte": "^2.35.12",
				"esrun": "^3.2.26",
				"gh-pages": "^6.1.1",
				"mdsvex": "^0.11.0",
				"prettier": "^3.1.1",
				"prettier-plugin-svelte": "^3.1.2",
				"remark": "^15.0.1",
				"remark-containers": "^1.2.0",
				"remark-github": "^12.0.0",
				"remark-toc": "^9.0.0",
				"sass": "^1.71.1",
				"simple-icons": "^11.7.0",
				"smui-theme": "^7.0.0-beta.16",
				"svelte": "^4.2.12",
				"svelte-check": "^3.6.0",
				"svelte-material-ui": "^7.0.0-beta.16",
				"svelte-portal": "^2.2.1",
				"swagger-ui": "^5.11.9",
				"tinygesture": "^3.0.0",
				"tslib": "^2.4.1",
				"typescript": "^5.0.0",
				"vite": "^5.0.3",
				"vite-plugin-static-copy": "^1.0.1",
				"vitest": "^1.2.0"
		},
		"type": "module"
}
